{
  "name": "@power/apm-client-js",
  "version": "0.12.0",
  "description": "@power/apm-client-js - APM Client SDK with Vue3 support",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ribuluo000/vue3-lib-boilerplate/issues"
  },
  "publishConfig": {
    "registry": "http://192.168.140.17:8081/nexus/repository/npm-private/"
  },
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./vue3": {
      "import": "./dist/vue3.mjs",
      "require": "./dist/vue3.js",
      "types": "./dist/vue3.d.ts"
    }
  },
  "files": [
    "README.md",
    "MIGRATION.md",
    "dist/"
  ],
  "keywords": [
    "power-apm",
    "apm",
    "monitoring",
    "vue3",
    "performance",
    "error-tracking"
  ],
  "scripts": {
    "dev": "vite",
    "build": "npm run build:lib && npm run build:types",
    "build:lib": "vite build",
    "build:types": "vue-tsc --declaration --emitDeclarationOnly",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,md}\"",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky install",
    "release": "standard-version",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vitest/ui": "^1.0.0",
    "@vue/test-utils": "^2.4.0",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jsdom": "^23.0.0",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "standard-version": "^9.5.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^1.0.0",
    "vue": "^3.3.0",
    "vue-tsc": "^1.8.0"
  },
  "dependencies": {
    "js-base64": "^3.7.0"
  },
  "peerDependencies": {
    "vue": "^2.6.0 || ^3.0.0"
  },
  "peerDependenciesMeta": {
    "vue": {
      "optional": true
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  }
}
